{
  "name": "web-audio-scheduler",
  "description": "Event Scheduler for Web Audio API",
  "version": "1.0.0",
  "author": "Nao Yonamine <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/web-audio-scheduler/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-eslint": "^4.1.5",
    "browserify": "^9.0.8",
    "eslint": "^1.9.0",
    "espower-babel": "^3.3.0",
    "isparta": "^3.5.3",
    "mocha": "^2.3.3",
    "power-assert": "^1.2.0",
    "sinon": "^1.17.2",
    "tickable-timer": "^0.3.0",
    "uglify-js": "^2.6.0",
    "web-audio-test-api": "^0.2.1"
  },
  "files": [
    "package.json",
    "README.md",
    "index.js",
    "lib"
  ],
  "homepage": "https://github.com/mohayonao/web-audio-scheduler/",
  "keywords": [
    "WebAudioAPI",
    "event",
    "scheduler",
    "timeline"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohayonao/web-audio-scheduler.git"
  },
  "scripts": {
    "build": "npm run build:to5 && npm run build:browser && npm run build:minify",
    "build:browser": "browserify index.js --standalone WebAudioScheduler --outfile build/web-audio-scheduler.js",
    "build:minify": "uglifyjs build/web-audio-scheduler.js -o build/web-audio-scheduler.min.js",
    "build:to5": "babel src --out-dir lib",
    "cover": "babel-node $(npm bin)/isparta cover --report text --report html _mocha",
    "lint": "eslint src test",
    "prepublish": "rm -rf lib && npm run lint && npm run test && npm run build:to5",
    "test": "mocha --compilers js:espower-babel/guess",
    "travis": "npm run lint && npm run test"
  }
}
